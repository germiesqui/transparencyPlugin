{
    "name": "Transparency Plugin",
    "version": "3.4",
    "description": "Extension que analiza noticias y provee de accesibilidad a dicha noticia.",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "content_security_policy": "script-src 'self' https://code.jquery.com/ https://cdnjs.cloudflare.com/ajax/libs/popper.js/ https://stackpath.bootstrapcdn.com/bootstrap/ https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.9.3/Chart.bundle.min.js 'unsafe-eval'; object-src 'self'"
}